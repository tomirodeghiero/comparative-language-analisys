program ErrorTypeInExecutionTime;

var
  a: Integer;
  b: Real;

begin
  Write('Ingresa un número entero: ');
  ReadLn(a);  // The user must enter an integer number

  Write('Ingresa un número real: ');
  ReadLn(b); // The user must enter a real number

  WriteLn('El valor entero es: ', a);
  WriteLn('El valor real es: ', b:0:2);
end.
